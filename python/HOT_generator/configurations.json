{
    "description":"Simple template to create a VM",
	"resources":
    [
	    {
			"resource_name":"VM1",
			"type":"OS::Nova::Server",
			"properties":
			{
				"name":"test_VM2",
				"image":"IMS",
				"flavor":"m1.medium",
				"avialability_zone":"nova",
				"security_groups":
				[
					{
						"name":"default",
						"resource_flag":"false"
					},
					{
						"name":"security_group1",
						"resource_flag":"true"
					}
				],
				"networks":
				[
					{
						"name":"port1",
						"resource_flag":"true",
						"port_flag":"true"
						
					}
				]
			}
		},
		{
			"resource_name":"Net1",
			"type":"OS::Neutron::Net",
			"properties":
			{
				"name":"heat_network"
			}
		},
		{
			"resource_name":"subnet1",
			"type":"OS::Neutron::Subnet",
			"properties":
			{
				"name":"heat_subnet",
				"network_id":"Net1",
				"cidr":"192.168.202.0/24",
				"enable_dhcp":"False",
				"gateway_ip":"192.168.202.254",
				"allocation_pool_start":"192.168.202.10",
				"allocation_pool_end":"192.168.202.240",
				"resource_flag":"true"
			}
		},
		{
			"resource_name":"port1",
			"type":"OS::Neutron::Port",
			"properties":
			{
				"name":"port-heat_network",
				"network_id":"Net1",				
				"security_groups":
				[
					{
						"name":"default",
						"resource_flag":"false"
					},
					{
						"name":"security_group1",
						"resource_flag":"true"
					}
				]
			}
		},
		{
			"resource_name":"floating_ip1",
			"type":"OS::Neutron::FloatingIP",
			"properties":
			{
				"floating_network_id":"26c50edd-5d8e-40c2-ae35-d6edfdc7f958",
				"port_id":"port1"
			}
		},
		{
			"resource_name":"router1",
			"type":"OS::Neutron::Router",
			"properties":
			{
				"name":"tenant_router_202"
			}
		},
		{
			"resource_name":"router_gtateway",
			"type":"OS::Neutron::RouterGateway",
			"properties":
			{
				"router_id":"router1",
				"ext_network_id":"26c50edd-5d8e-40c2-ae35-d6edfdc7f958",
				"resource_flag":"true"
			}
		},
		{
			"resource_name":"router_interface",
			"type":"OS::Neutron::RouterInterface",
			"properties":
			{
				"router_id":"router1",
				"subnet_id":"subnet1",
				"resource_flag":"true"
			}
		},
		{
			"resource_name":"security_group1",
			"type":"OS::Neutron::SecurityGroup",
			"properties":
			{
				"name":"heat_security_grp",
				"description":"security group for testing",
				"rules":
				[
					{
						"ethertype":"IPv4",
						"direction":"egress",
						"protocol":"icmp",
						"port_range_min":"None",
						"port_range_max":"None",
						"remote_group_id":"None",
						"remote_ip_prefix":"None"
					},
					{
						"ethertype":"IPv4",
						"direction":"egress",
						"protocol":"tcp",
						"port_range_min":"22",
						"port_range_max":"22",
						"remote_group_id":"None",
						"remote_ip_prefix":"None"
					}
				]
				
			}
		}
    ]
}
